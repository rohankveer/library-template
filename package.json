{
  "name": "@rv/web-library-template",
  "version": "0.0.1",
  "type": "module",
  "peerDependencies": {
    "react": "^18.3.0",
    "react-dom": "^18.3.0"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "files": [
    "src/dist"
  ],
  "exports": {
    ".": {
      "dev": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "audit": "yarn npm audit --recursive --no-deprecations",
    "audit:production": "yarn audit --environment production",
    "clean": "rimraf -g '**/dist' '**/*.tsbuildinfo'",
    "compile": "tsc -b tsconfig.build.json",
    "lint": "concurrently -c auto 'yarn:lint:*'",
    "lint:prettier": "prettier . -c --log-level warn",
    "lint:scripts": "eslint . --report-unused-disable-directives --max-warnings=0",
    "lint:typescript": "yarn tsc --noemit",
    "fix": "yarn fix:scripts && yarn fix:prettier",
    "fix:prettier": "prettier . -w --log-level warn",
    "fix:scripts": "yarn run lint:scripts --fix",
    "storybook:start": "storybook dev --port 6006",
    "storybook:build": "storybook build",
    "storybook:deploy": "gh-pages -d storybook-static",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "storybook:test": "yarn test-storybook --url http://localhost:6006 --failOnConsole --maxWorkers=4",
    "storybook:test:serve": "concurrently -k -s first -c auto -n \"serve,test\" \"yarn storybook:serve\" \"wait-on tcp:6006 && yarn storybook:test\"",
    "test": "jest"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@emotion/core": "^10.3.1",
    "@storybook/addon-a11y": "8.1.5",
    "@storybook/addon-actions": "8.1.5",
    "@storybook/addon-essentials": "8.1.5",
    "@storybook/addon-interactions": "8.1.5",
    "@storybook/blocks": "8.1.5",
    "@storybook/react": "8.1.5",
    "@storybook/react-vite": "8.1.5",
    "@storybook/test-runner": "^0.18.2",
    "@storybook/theming": "8.1.5",
    "@storybook/types": "8.1.5",
    "@swc/core": "^1.5.22",
    "@swc/jest": "^0.2.36",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/identity-obj-proxy": "^3.0.2",
    "@types/jest": "^29.5.12",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.12.13",
    "@types/prop-types": "^15.7.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "gh-pages": "patch:gh-pages@npm%3A6.1.1#~/.yarn/patches/gh-pages-npm-6.1.1-7bde86cdf6.patch",
    "http-server": "^14.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fail-on-console": "^3.3.0",
    "jest-junit": "^16.0.0",
    "playwright": "^1.44.1",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "storybook": "8.1.5",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "wait-on": "^7.2.0"
  },
  "resolutions": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/node": "^20.0.0",
    "@changesets/cli@^2.27.1": "patch:@changesets/cli@npm%3A^2.27.1#~/.yarn/patches/@changesets-cli-npm-2.27.1-2cfd25f1c6.patch",
    "@changesets/get-github-info@npm:^0.6.0": "patch:@changesets/get-github-info@npm%3A^0.6.0#~/.yarn/patches/@changesets-get-github-info-npm-0.6.0-6c129e4581.patch"
  },
  "packageManager": "yarn@4.2.1"
}
